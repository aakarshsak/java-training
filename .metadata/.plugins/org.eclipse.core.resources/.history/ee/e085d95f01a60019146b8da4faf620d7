package com.example;

public class ArrayDemo {

	public static void main(String[] args) {
		
		
		
		int s[] = {10, 20, 30, 40, 50};
		for(int x: s) {
			System.out.println(x);
		}
		System.out.println("\n\n");
		int d[] = new int[10];
		for(int x: d) {
			System.out.println(x);
		}
		System.out.println("\n\n");
		System.arraycopy(s, 2, d, 5, 3);
		for(int x: d) {
			System.out.println(x);
		}
	}

}
